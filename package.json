{
  "name": "vue-ssr",
  "version": "0.0.1",
  "private": true,
  "description": "try to learn using vue with server slide render deeply",
  "repository": "git@github.com:JounQin/vue-ssr.git",
  "author": "JounQin <admin@1stg.me>",
  "license": "MIT",
  "now": {
    "alias": "vue-ssr"
  },
  "scripts": {
    "dev": "cross-env DEBUG=hi:* node build/bin/start",
    "start": "cross-env NODE_ENV=production npm run dev",
    "build": "rimraf dist && npm run compile",
    "prod": "npm run build && npm run start",
    "compile:base": "npm run compile:client && npm run compile:server",
    "compile:dev": "cross-env DEBUG=hi:* npm run compile:base",
    "compile": "cross-env NODE_ENV=production npm run compile:dev",
    "compile:client": "webpack --config build/webpack/client.babel.js --progress --hide-modules",
    "compile:server": "webpack --config build/webpack/server.babel.js --progress --hide-modules",
    "lint": "npm run lint:es",
    "lint:fix": "npm run lint:es -- --fix",
    "lint:es": "eslint --ext .js,.vue --max-warnings 10 .",
    "pm2": "cross-env NODE_ENV=production DEBUG=hi:* pm2 start build/bin/start.js --name=vue-ssr",
    "deploy": "bash build/bash/deploy.sh"
  },
  "dependencies": {
    "axios": "^0.17.0",
    "highlight.js": "^9.12.0",
    "html-minifier": "^3.5.6",
    "koa": "^2.3.0",
    "koa-compress": "^2.0.0",
    "koa-logger": "^3.1.0",
    "koa-router": "^7.2.1",
    "koa-static": "^4.0.1",
    "lodash": "^4.17.4",
    "lru-cache": "^4.1.1",
    "markdown-it": "^8.4.0",
    "mkdirp": "^0.5.1",
    "normalize.css": "^7.0.0",
    "path-to-regexp": "^2.0.0",
    "vue": "^2.5.2",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.2",
    "vuex": "^3.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.2",
    "babel-loader": "^7.1.2",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "chokidar": "^2.0.0",
    "cross-env": "^5.1.0",
    "css-loader": "^0.28.7",
    "debug": "^3.1.0",
    "eslint": "^4.9.0",
    "eslint-config-1stg": "1.0.1",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "img-loader": "^2.0.0",
    "lodash-webpack-plugin": "^0.11.4",
    "null-loader": "^0.1.1",
    "pug": "^2.0.0-rc.4",
    "rimraf": "^2.6.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "sw-precache-webpack-plugin": "^0.11.4",
    "url-loader": "^0.6.2",
    "vconsole": "^3.0.0",
    "vue-loader": "^13.3.0",
    "vue-style-loader": "^3.0.3",
    "vue-template-compiler": "^2.5.2",
    "webpack": "^3.8.1",
    "webpack-dev-middleware": "^2.0.1",
    "webpack-hot-middleware": "^2.20.0",
    "webpack-node-externals": "^1.6.0",
    "yargs": "^11.0.0"
  }
}
