{
  "name": "vue-ssr",
  "version": "0.0.1",
  "private": true,
  "description": "try to learn using vue with server slide render deeply",
  "repository": "git@github.com:JounQin/vue-ssr.git",
  "author": "JounQin <admin@1stg.me>",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env DEBUG=hi:* node build/bin/start",
    "prod": "cross-env NODE_ENV=production npm run dev",
    "start": "npm run build && npm run prod",
    "build": "rimraf dist && npm run compile",
    "compile:base": "npm run compile:client && npm run compile:server",
    "compile:dev": "cross-env DEBUG=hi:* npm run compile:base",
    "compile": "cross-env NODE_ENV=production npm run compile:dev",
    "compile:client": "webpack --config build/webpack/client.babel.js --progress --hide-modules",
    "compile:server": "webpack --config build/webpack/server.babel.js --progress --hide-modules",
    "lint": "npm run lint:es",
    "lint:fix": "npm run lint:es -- --fix",
    "lint:es": "eslint --ext .js,.vue --max-warnings 10 .",
    "pm2": "cross-env NODE_ENV=production DEBUG=hi:* pm2 start build/bin/start.js --name=vue-ssr"
  },
  "dependencies": {
    "bootstrap-styl": "^5.0.8",
    "fonts.css": "^1.5.0",
    "koa": "^2.0.0",
    "koa-compress": "^2.0.0",
    "koa-logger": "^2.0.1",
    "koa-static": "^3.0.0",
    "lodash": "^4.17.4",
    "lru-cache": "^4.0.2",
    "nib": "^1.1.2",
    "stylus-px2rem": "^1.0.11",
    "vue": "^2.1.10",
    "vue-router": "^2.2.0",
    "vue-server-renderer": "^2.1.10",
    "vue-ssr-html-stream": "^1.1.0",
    "vuex": "^2.1.1"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.22.0",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "debug": "^2.6.0",
    "eslint": "^3.15.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-babel": "^4.0.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "eslint-plugin-vue": "^2.0.1",
    "eslint-plugin-vuefix": "^0.1.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.2",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.28.0",
    "img-loader": "^1.3.1",
    "pug": "^2.0.0-beta9",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.4.0",
    "sw-precache-webpack-plugin": "^0.8.0",
    "url-loader": "^0.5.7",
    "vue-loader": "^10.3.0",
    "vue-ssr-webpack-plugin": "^1.0.1",
    "vue-style-loader": "^2.0.0",
    "vue-template-compiler": "^2.1.10",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.16.1",
    "yargs": "^6.6.0"
  }
}
