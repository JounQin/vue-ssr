{
  "name": "vue-ssr",
  "version": "0.0.1",
  "private": true,
  "description": "try to learn using vue with server slide render deeply",
  "repository": "git@github.com:JounQin/vue-ssr.git",
  "author": "JounQin <admin@1stg.me>",
  "license": "MIT",
  "now": {
    "alias": "vue-ssr"
  },
  "scripts": {
    "dev": "cross-env DEBUG=hi:* NODE_ENV=development babel-node server",
    "start": "cross-env DEBUG=hi:* NODE_ENV=production node dist/server",
    "build:server": "webpack --config build/server.babel.js --color --progress",
    "build:template": "mkdir -p dist/static && babel-node build/template",
    "build:vue:client": "webpack --config build/vue-client.babel.js --color --progress",
    "build:vue:server": "webpack --config build/vue-server.babel.js --color --progress",
    "build": "rimraf dist && cross-env NODE_ENV=production run-p build:server build:template build:vue:client build:vue:server",
    "sync:pull": "bash build/sync-pull.sh",
    "sync:push": "bash build/sync-push.sh",
    "lint": "npm run lint:es",
    "lint:fix": "npm run lint:es -- --fix",
    "lint:es": "eslint --ext .js,.vue --max-warnings 10 .",
    "precommit": "lint-staged",
    "pm2": "cross-env DEBUG=hi:* NODE_ENV=production pm2 start build/bin/start.js --name=vue-ssr",
    "prune": "yarn install --production --ignore-optional --ignore-scripts --prefer-offline --force"
  },
  "babel": {
    "presets": [
      "@babel/env",
      [
        "@babel/stage-0",
        {
          "decoratorsLegacy": true
        }
      ]
    ]
  },
  "eslintIgnore": [
    "dist"
  ],
  "lint-staged": {
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,vue}": [
      "eslint --max-warnings=10 --fix",
      "git add"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "overrides": [
      {
        "files": "*.json",
        "options": {
          "printWidth": 150
        }
      }
    ]
  },
  "dependencies": {
    "axios": "^0.18.0",
    "cross-env": "^5.1.6",
    "debug": "^3.1.0",
    "highlight.js": "^9.12.0",
    "html-minifier": "^3.5.16",
    "koa": "^2.5.1",
    "koa-compress": "^3.0.0",
    "koa-logger": "^3.2.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "lodash": "^4.17.10",
    "lodash-es": "^4.17.10",
    "lru-cache": "^4.1.3",
    "markdown-it": "^8.4.1",
    "mkdirp": "^0.5.1",
    "normalize.css": "^8.0.0",
    "path-to-regexp": "^2.2.1",
    "vue": "^2.5.16",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.16",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.49",
    "@babel/node": "^7.0.0-beta.49",
    "@babel/polyfill": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "@babel/preset-stage-0": "^7.0.0-beta.49",
    "@babel/register": "^7.0.0-beta.49",
    "babel-eslint": "^8.2.3",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^8.0.0-beta.3",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "css-loader": "^0.28.11",
    "eslint": "^5.0.0",
    "eslint-config-1stg": "^2.1.0",
    "file-loader": "^1.1.11",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "img-loader": "^3.0.0",
    "koa-webpack": "^5.0.2",
    "lint-staged": "^7.1.3",
    "mini-css-extract-plugin": "^0.4.0",
    "npm-run-all": "^4.1.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.13.4",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "rimraf": "^2.6.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "sw-precache-webpack-plugin": "^0.11.5",
    "url-loader": "^1.0.1",
    "vue-loader": "^15.2.4",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.11.0",
    "webpack-cli": "^3.0.2",
    "webpack-merge": "^4.1.2",
    "webpack-node-externals": "^1.7.2"
  }
}
