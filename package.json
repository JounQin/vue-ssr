{
  "name": "vue-ssr",
  "version": "0.0.1",
  "private": true,
  "description": "try to learn using vue with server slide render deeply",
  "repository": "git@github.com:JounQin/vue-ssr.git",
  "author": "JounQin <admin@1stg.me>",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env DEBUG=hi:* node build/bin/start",
    "prod": "cross-env NODE_ENV=production npm run dev",
    "start": "npm run build && npm run prod",
    "build": "rimraf dist && npm run compile",
    "compile:base": "npm run compile:client && npm run compile:server",
    "compile:dev": "cross-env DEBUG=hi:* npm run compile:base",
    "compile": "cross-env NODE_ENV=production npm run compile:dev",
    "compile:client": "webpack --config build/webpack/client.babel.js --progress --hide-modules",
    "compile:server": "webpack --config build/webpack/server.babel.js --progress --hide-modules",
    "lint": "npm run lint:es",
    "lint:fix": "npm run lint:es -- --fix",
    "lint:es": "eslint --ext .js,.vue --max-warnings 10 .",
    "pm2": "cross-env NODE_ENV=production DEBUG=hi:* pm2 start build/bin/start.js --name=vue-ssr",
    "deploy": "bash build/bash/deploy.sh"
  },
  "dependencies": {
    "axios": "^0.16.1",
    "bootstrap-styl": "^5.0.8",
    "fonts.css": "^1.5.0",
    "koa": "^2.2.0",
    "koa-compress": "^2.0.0",
    "koa-logger": "^2.0.1",
    "koa-router": "^7.1.1",
    "koa-static": "^3.0.0",
    "lodash": "^4.17.4",
    "lru-cache": "^4.0.2",
    "nib": "^1.1.2",
    "stylus-px2rem": "^1.0.11",
    "vue": "^2.3.0",
    "vue-router": "^2.5.2",
    "vue-server-renderer": "^2.3.0",
    "vuex": "^2.3.1"
  },
  "devDependencies": {
    "babel-core": "^7.0.0-alpha.9",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-polyfill": "^7.0.0-alpha.9",
    "babel-preset-env": "^2.0.0-alpha.7",
    "babel-preset-stage-0": "^7.0.0-alpha.9",
    "babel-register": "^7.0.0-alpha.9",
    "babili-webpack-plugin": "^0.0.11",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.0-beta.0",
    "css-loader": "^0.28.0",
    "debug": "^2.6.4",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^2.0.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "img-loader": "^2.0.0",
    "lodash-webpack-plugin": "^0.11.3",
    "null-loader": "^0.1.1",
    "pug": "^2.0.0-beta12",
    "rimraf": "^2.6.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "sw-precache-webpack-plugin": "^0.10.0",
    "url-loader": "^0.5.8",
    "vconsole": "^2.5.2",
    "vue-loader": "^12.0.0",
    "vue-ssr-webpack-plugin": "^3.0.0",
    "vue-style-loader": "^3.0.1",
